	<subDataset name="TableData" uuid="a10c719c-b9f0-4b23-9faf-c92e808ce880">
		<parameter name="fromdate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="todate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="tanggal" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="co_id" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select A.co_id, A.so_num, h.inv_code, A.tanggal, A.tgl_jt_kirim, C.cust_nama ,D.wilayah_id,D.wilayah_nama ,A.released_date, A.status, J.qty, E.prod_nama
    , F.spesifikasi_nama, B.hrg_loco, j.harga, B.n_netto, B.sat_id, G.lokasi_nama, A.tgl_jt_kirim, k.sales_nama
    from so0 A
    left join (select co_id,so_num, prod_id,sat_id, sum(qty) as qty, sum(hrg_loco) as hrg_loco, sum(hrg) as harga, sum(n_netto) as n_netto from so10 group by co_id, so_num, prod_id, sat_id) B on A.co_id=B.co_id and A.so_num=B.so_num
    left join customer C on A.co_id=C.co_id and A.cust_id=C.cust_id
    left join wilayah D on A.co_id=D.co_id and A.wilayah_id=D.wilayah_id
    left join items E on B.prod_id=E.prod_id
    left join spesifikasi F on E.spesifikasi_id=F.spesifikasi_id and E.co_id=F.co_id
    left join so11 G on B.co_id=G.co_id and B.so_num=G.so_num and B.prod_id=G.prod_id and B.sat_id=G.sat_id
    inner join ar_sale H on a.so_num=h.so_num and a.co_id=h.co_id
    left join ar_sale_detail J on h.inv_code=j.inv_code and h.co_id=j.co_id
    left join salesman K on a.sales_id=k.sales_id and a.co_id = k.co_id
where a.co_id=$P{co_id} and A.tanggal between $P{fromdate} and $P{todate}]]>
		</queryString>
		<field name="CO_ID" class="java.lang.String"/>
		<field name="SO_NUM" class="java.lang.String"/>
		<field name="INV_CODE" class="java.lang.String"/>
