	<parameter name="co_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="inv_code" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select a.co_id, a.inv_code, b.so_num, d.cust_nama, d.alamat, d.kota, d.telepon1, d.fax,
a.posted_date, b.prod_id,f.prod_nama, e.qty, e.harga, e.qty * e.harga as total,e.sat_id
from ar_sale a
inner join deliveryorder b on a.co_id=b.co_id and a.do_num=b.do_num
inner join so0 c on b.so_num=c.so_num and b.co_id=c.co_id
left join customer d on c.cust_id=d.cust_id and c.co_id=d.co_id
left join ar_sale_detail e on a.inv_code=e.inv_code and a.co_id=e.co_id
left join items f on b.prod_id=f.prod_id and b.co_id=f.co_id
where a.co_id=$P{co_id} and a.inv_code=$P{inv_code}]]>
	</queryString>
