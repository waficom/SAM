	<subDataset name="TableData" uuid="f5502d27-7e20-4171-a869-c8077266080b">
		<parameter name="fromdate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="todate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select A.inv_code, B.inv_code, A.cust_id, C.cust_nama, E.debit as jml_tagihan, F.credit as tagihan_rev,
K.credit as pembayaran, L.debit as pembayaran_rev,
(E.debit - F.credit) -(K.credit-L.debit) as SISA
from ar_sale A
left join ar_sale B on A.inv_code=B.inv_code_revisi and B.inv_date between $P{fromdate} and $P{todate}
left join customer C on A.cust_id=C.cust_id and A.co_id=C.co_id
left join customer D on B.cust_id=D.cust_id and B.co_id=D.co_id
left join jurnal E on A.inv_code=E.inv_code and C.coa_arp=E.coa
left join jurnal F on B.inv_code=F.inv_code and D.coa_arp=F.coa
/*pembayaran*/
left join ar_sale_payment G on A.inv_code=G.for_inv_code and A.co_id=G.co_id and G.inv_date between $P{fromdate} and $P{todate}
left join ar_sale_payment H on G.inv_code=H.inv_code_revisi and G.co_id=H.co_id and H.inv_date between $P{fromdate} and $P{todate}
left join customer I on G.cust_id=I.cust_id and G.co_id=I.co_id
left join customer J on H.cust_id=J.cust_id and H.co_id=J.co_id
left join jurnal K on G.inv_code=K.inv_code and I.coa_arp=K.coa
left join jurnal L on H.inv_code=L.inv_code and J.coa_arp=L.coa
where A.inv_type='N' and A.inv_date between $P{fromdate} and $P{todate}]]>
		</queryString>
		<field name="INV_CODE" class="java.lang.String"/>
		<field name="CUST_ID" class="java.lang.String"/>
		<field name="CUST_NAMA" class="java.lang.String"/>
