		<field name="TAGIHAN_REV" class="java.math.BigDecimal"/>
		<field name="PEMBAYARAN" class="java.math.BigDecimal"/>
		<field name="PEMBAYARAN_REV" class="java.math.BigDecimal"/>
		<field name="SISA" class="java.math.BigDecimal"/>
