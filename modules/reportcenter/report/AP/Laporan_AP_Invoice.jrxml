	<subDataset name="TableData" uuid="f5502d27-7e20-4171-a869-c8077266080b">
		<parameter name="fromdate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="todate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select A.inv_code, A.inv_date, B.qty, B.harga, A.discon, A.ppn_prs, A.pph_prs, (B.qty * B.harga) as nilaidasar
, (B.qty * B.harga) - (A.discon / 100) as nd_setelah_discon, C.vend_nama, A.vend_id, A.ppn_prs, A.pph_prs
from ap_inv A
left join ap_inv_detail B on A.co_id=B.co_id and A.inv_code=B.inv_code
left join vendor C on A.vend_id=C.vend_id and A.co_id=C.co_id
where A.status=1 and A.inv_date between $P{fromdate} and $P{todate}]]>
		</queryString>
		<field name="INV_CODE" class="java.lang.String"/>
		<field name="INV_DATE" class="java.sql.Timestamp"/>
		<field name="QTY" class="java.lang.Integer"/>
		<field name="HARGA" class="java.math.BigDecimal"/>
		<field name="DISCON" class="java.math.BigDecimal"/>
		<field name="PPN_PRS" class="java.math.BigDecimal"/>
