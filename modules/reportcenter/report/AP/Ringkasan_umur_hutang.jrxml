		<queryString>
			<![CDATA[select a.co_id, a.periode, a.vend_id, a.vend_nama, sum(a.periode30) as hari30, sum(a.periode60) as hari60
,sum(a.periode90) as hari90, sum(a.periode120) as hari120, sum(a.periode150) as hari150, sum(a.periode180) as hari180
, sum(a.periode181) as hari181
from view_dftr_umur_hutang a
where a.co_id=$P{co_id} and a.periode=$P{periode}
group by  a.co_id, a.periode, a.vend_id, a.vend_nama]]>
