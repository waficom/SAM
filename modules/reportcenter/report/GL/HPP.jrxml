	<queryString>
		<![CDATA[select C.co_id, C.periode, null as account, 'Persediaan Awal bahan Baku' as Description,
sum(C.qty_awal) as Qty, sum(C.total_awal) as Jumlah, null as Jumlah2, 'BB' as JobDesc, 1 as urut
from inventory_bb C
group by C.co_id , C.periode

union all

select C.co_id, C.periode, null as account, 'Pembelian bahan Baku' as Description,
sum(C.qty_masuk) as Qty, sum(C.total_masuk) as Jumlah,null as Jumlah2, 'BB' as JobDesc , 2 as urut
from inventory_bb C
group by C.co_id, C.periode

union all

select C.co_id, C.periode, null as account, 'Persediaan Akhir bahan Baku' as Description,
sum(C.qty_akhir) as Qty, sum(C.total_akhir) as Jumlah,null as Jumlah2, 'BB' as JobDesc , 3 as urut
from inventory_bb C
group by C.co_id, C.periode

union all

select C.co_id, C.periode, null as account, 'Pemakaian bahan Baku' as Description,
sum(C.qty_keluar) as Qty, sum(C.total_keluar) as Jumlah,null as Jumlah2, 'BB' as JobDesc, 4 as urut
from inventory_bb C
group by C.co_id, C.periode

union all

select B.co_id, cast(extract(year from B.posted_date) as varchar(4)) || sright('00' || cast(extract(month from B.posted_date) as varchar(2)),2) as periode,
B.account, B1.coa_nama as Description, null as Qty, null as Jumlah, sum(B.debit) as Jumlah2, 'RC' as JobDesc
, 5 as urut
from ap_reclass B
left join coa B1 on B.account=B1.coa_id and B.co_id=B1.co_id
where B.inv_type='OVB'
group by B.co_id, periode,  B.account, B1.coa_nama

union all

select C.co_id, C.periode, null as account, 'Persediaan Awal Barang dalam Proses' as Description,
null as Qty, null as Jumlah,null as Jumlah2, 'BD' as JobDesc, 6 as urut
from inventory_bb C
group by C.co_id, C.periode

union all

select C.co_id, C.periode, null as account, 'Persediaan Akhir Barang dalam Proses' as Description,
null as Qty, null as Jumlah,null as Jumlah2, 'BD' as JobDesc , 7 as urut
from inventory_bb C
group by C.co_id, C.periode

union all

select D.co_id, D.periode, null as account, 'Persedian Awal Barang Jadi' as Description,
sum(D.qty_awal) as Qty, sum(D.total_awal) as Jumlah,null as Jumlah2, 'BJ' as JobDesc, 8 as urut
from inventory_items D
group by D.co_id, D.periode

union all

select D.co_id, D.periode, null as account, 'Barang Tersedia untuk di Jual' as Description,
sum(D.qty_masuk) as Qty, sum(D.total_masuk) as Jumlah,null as Jumlah2, 'BJ' as JobDesc, 9 as urut
from inventory_items D
group by D.co_id, D.periode

union all

select D.co_id, D.periode, null as account, 'Persedian Akhir Barang Jadi' as Description,
sum(D.qty_akhir) as Qty, sum(D.total_akhir) as Jumlah,null as Jumlah2, 'BJ' as JobDesc, 10 as urut
from inventory_items D
group by D.co_id, D.periode]]>
	</queryString>
