	<subDataset name="TableData" uuid="f5502d27-7e20-4171-a869-c8077266080b">
		<parameter name="co_id" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="periode" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select * from view_harga_pokok
where co_id=$P{co_id} and periode=$P{periode}
order by periode, urut, account ASC]]>
		</queryString>
		<field name="CO_ID" class="java.lang.String"/>
		<field name="PERIODE" class="java.lang.String"/>
		<field name="ACCOUNT" class="java.lang.String"/>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="QTY" class="java.math.BigDecimal"/>
		<field name="JUMLAH" class="java.math.BigDecimal"/>
		<field name="JUMLAH2" class="java.math.BigDecimal"/>
		<field name="JOBDESC" class="java.lang.String"/>
		<field name="URUT" class="java.lang.Integer"/>
		<variable name="AUTONUMBER" class="java.lang.Integer" calculation="Count">
			<variableExpression><![CDATA[$V{AUTONUMBER}]]></variableExpression>
			<initialValueExpression><![CDATA[new Integer(1)]]></initialValueExpression>
		</variable>
		<variable name="sumjumlah" class="java.math.BigDecimal" resetType="Group" resetGroup="jobdesc" calculation="Sum">
			<variableExpression><![CDATA[($F{JUMLAH} != null ? $F{JUMLAH} : new BigDecimal("0.00"))]]></variableExpression>
		</variable>
		<variable name="sumjumlah2" class="java.math.BigDecimal" resetType="Group" resetGroup="jobdesc" calculation="Sum">
			<variableExpression><![CDATA[($F{JUMLAH2}!= null ? $F{JUMLAH2} : new BigDecimal("0.00"))]]></variableExpression>
		</variable>
		<variable name="totaljumlah" class="java.math.BigDecimal" resetType="Group" resetGroup="jobdesc">
			<variableExpression><![CDATA[$V{sumjumlah2}.add($V{sumjumlah2}.add($V{sumjumlah}))]]></variableExpression>
		</variable>
		<group name="jobdesc">
			<groupExpression><![CDATA[$F{JOBDESC}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="co_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
