	<subDataset name="dstotal" uuid="f70acb6a-00ef-43da-94c3-064e0c6a9c63">
		<parameter name="prd" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="coid" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
    ((SELECT coalesce(sum(tb.ending_balance),0)
    FROM COA c
    LEFT OUTER JOIN COA c1 ON c.CO_ID = c1.CO_ID AND c.COA_PARENT = c1.COA_ID
    LEFT OUTER JOIN GET_SUM_NERACA($P{coid}, c.coa_id, $P{prd}) tb ON c.CO_ID = tb.CO_ID AND c.COA_ID = tb.COA_ID
    WHERE c.CO_ID = $P{coid} and SUBSTR(c.jenis,1,1) = 'R' and c.status = 'Y') -

    (SELECT coalesce(sum(tb.ending_balance),0)
    FROM COA c
    LEFT OUTER JOIN COA c1 ON c.CO_ID = c1.CO_ID AND c.COA_PARENT = c1.COA_ID
    LEFT OUTER JOIN GET_SUM_NERACA($P{coid}, c.coa_id, $P{prd}) tb ON c.CO_ID = tb.CO_ID AND c.COA_ID = tb.COA_ID
    WHERE c.CO_ID = $P{coid} and SUBSTR(c.jenis,1,1) = 'E' and c.status = 'Y')) as rl,

    ((SELECT coalesce(sum(tb.ytd),0)
    FROM COA c
    LEFT OUTER JOIN COA c1 ON c.CO_ID = c1.CO_ID AND c.COA_PARENT = c1.COA_ID
    LEFT OUTER JOIN GET_SUM_LR_YTD($P{coid}, c.coa_id, $P{prd}) tb ON c.CO_ID = tb.CO_ID AND c.COA_ID = tb.COA_ID
    WHERE c.CO_ID = $P{coid} and SUBSTR(c.jenis,1,1) = 'R' and c.status = 'Y') -

    (SELECT coalesce(sum(tb.ytd),0)
    FROM COA c
    LEFT OUTER JOIN COA c1 ON c.CO_ID = c1.CO_ID AND c.COA_PARENT = c1.COA_ID
    LEFT OUTER JOIN GET_SUM_LR_YTD($P{coid}, c.coa_id, $P{prd}) tb ON c.CO_ID = tb.CO_ID AND c.COA_ID = tb.COA_ID
    WHERE c.CO_ID = $P{coid} and SUBSTR(c.jenis,1,1) = 'E' and c.status = 'Y')) as ytd
from rdb$database]]>
		</queryString>
		<field name="RL" class="java.math.BigDecimal"/>
		<field name="YTD" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="coid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="prd" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/home/dharma/PhpstormProjects/SAM/modules/reportcenter/report/GL/"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT c.*, c1.coa_nama as cp_nama, tb.periode, coalesce(tb.debit,0) as debit, coalesce(tb.credit,0) as credit,
coalesce(tb.incdec,0) as incdec,
coalesce(y.debit,0) as debit_ytd,
coalesce(y.credit,0) as credit_ytd,
coalesce(y.incdec,0) as incdec_ytd
FROM COA c
LEFT OUTER JOIN COA c1 ON c.CO_ID = c1.CO_ID AND c.COA_PARENT = c1.COA_ID
LEFT OUTER JOIN GET_MUTASI_LR($P{coid}, c.coa_id, $P{prd}) tb ON c.CO_ID = tb.CO_ID AND c.COA_ID = tb.COA_ID
LEFT OUTER JOIN GET_MUTASI_LR_YTD($P{coid}, c.coa_id, $P{prd}) y ON c.CO_ID = y.CO_ID AND c.COA_ID = y.COA_ID
WHERE c.CO_ID = $P{coid} AND c.COA_PARENT is null
  AND ((SUBSTR(c.jenis,1,1) = 'R') OR (SUBSTR(c.jenis,1,1) = 'E'))
ORDER BY c.COA_ID]]>
	</queryString>
