	<subDataset name="TableData" uuid="a10c719c-b9f0-4b23-9faf-c92e808ce880">
		<parameter name="fromdate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="todate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="tanggal" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="co_id" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select A.co_id, A.po_num, A.tgl, A.tgl_jt, A.inv_num, C.bb_nama, B.qty, B.sat_id, B.hrg, B.n_brutto, B.disc_prs, B.n_netto, B.keterangan, D.vend_nama
from PO0 A
left join PO1 B on A.co_id=B.co_id and A.po_num=B.po_num
left join bahanbaku C on B.bb_id=C.bb_id and B.co_id=C.co_id
left join vendor D on A.vend_id=D.vend_id and A.co_id=D.co_id
where A.tgl between $P{fromdate} and $P{todate} and a.co_id=$P{co_id}]]>
		</queryString>
		<field name="CO_ID" class="java.lang.String"/>
		<field name="PO_NUM" class="java.lang.String"/>
		<field name="TGL" class="java.sql.Date"/>
		<field name="TGL_JT" class="java.sql.Date"/>
		<field name="INV_NUM" class="java.lang.String"/>
		<field name="BB_NAMA" class="java.lang.String"/>
		<field name="QTY" class="java.math.BigDecimal"/>
		<field name="SAT_ID" class="java.lang.String"/>
		<field name="HRG" class="java.math.BigDecimal"/>
		<field name="N_BRUTTO" class="java.math.BigDecimal"/>
		<field name="DISC_PRS" class="java.lang.Integer"/>
		<field name="N_NETTO" class="java.math.BigDecimal"/>
		<field name="KETERANGAN" class="java.lang.String"/>
		<field name="VEND_NAMA" class="java.lang.String"/>
		<variable name="AUTONUMBER" class="java.lang.Integer" calculation="Count">
			<variableExpression><![CDATA[$V{AUTONUMBER}]]></variableExpression>
			<initialValueExpression><![CDATA[new Integer(1)]]></initialValueExpression>
		</variable>
		<variable name="total" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{QTY}.multiply($F{HRG})]]></variableExpression>
		</variable>
		<variable name="total_qty" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{QTY}]]></variableExpression>
		</variable>
		<variable name="sum_total" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$V{total}]]></variableExpression>
		</variable>
		<variable name="sum_grandtotal" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{N_NETTO}]]></variableExpression>
		</variable>
